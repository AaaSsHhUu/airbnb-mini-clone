<% layout("./layouts/boilerplate.ejs") -%>
<body>
    <h2>All Listings</h2>
    <form action="/listnings/new" method="get">
        <button>Add new</button>
    </form>
    <ul>
        <% for(listing of allListings){ %>
            <li><a href="/listings/<%= listing._id %>"><%= listing.title %></a></li>
        <% } %>
    </ul>
</body>